CREATE TABLE ADUENVIOSALIDA
(
  ID_ADUENVIOSALIDA Integer NOT NULL,
  CODIGO_ADUMANIFIESTO MANIFIESTO NOT NULL,
  USERNAME DUSER DEFAULT USER,
  USERTIME AHORA DEFAULT 'now',
  CODI_ADUAN VARCHAR(3),
  ANO_ORDEN VARCHAR(4),
  NUME_ORDEN VARCHAR(6),
  ANO_PRESE VARCHAR(4),
  TIPO_CNT VARCHAR(1),
  NUME_CNT VARCHAR(15),
  ANO_DOCASO VARCHAR(4),
  CODI_REGI VARCHAR(2),
  CODI_OPERA VARCHAR(2),
  NUM_DOCASO VARCHAR(6),
  TIPO_DOCUM VARCHAR(1),
  LIBR_TRIBU VARCHAR(11),
  PESO_NETO PESO DEFAULT 0.00,
  TARA PESO DEFAULT 0.00,
  N_PRE_ADUA VARCHAR(15),
  N_PRE_OTRO VARCHAR(15),
  TAMANO INTEGER,
  TIPO_INGRE VARCHAR(1),
  CODI_CONDI VARCHAR(3),
  DESC_MERC VARCHAR(250),
  NAVE VARCHAR(50),
  ANO_MANIF VARCHAR(4),
  NUME_MANIF VARCHAR(5),
  MOTI_ANULA VARCHAR(250),
  FECH_SALI INTEGER,
  HORA_SALI VARCHAR(8),
  ADU_DOCASO VARCHAR(3),
  PAIS_DESTI VARCHAR(2),
  COD_ANULA VARCHAR(3),
  ULT_TRANS VARCHAR(2),
  CONSTRAINT PK_ADUENVIOSALIDA PRIMARY KEY (ID_ADUENVIOSALIDA)
);

ALTER TABLE ADUENVIOSALIDA ADD
  FOREIGN KEY (CODIGO_ADUMANIFIESTO) REFERENCES ADUMANIFIESTO (CODIGO_ADUMANIFIESTO) ON UPDATE CASCADE;

GRANT DELETE, INSERT, REFERENCES, SELECT, UPDATE
 ON ADUENVIOSALIDA TO ROLE ARCHIVO;

GRANT DELETE, INSERT, REFERENCES, SELECT, UPDATE
 ON ADUENVIOSALIDA TO ROLE BALANZA;

GRANT DELETE, INSERT, REFERENCES, SELECT, UPDATE
 ON ADUENVIOSALIDA TO ROLE CONTABILIDAD;

GRANT DELETE, INSERT, REFERENCES, SELECT, UPDATE
 ON ADUENVIOSALIDA TO ROLE GERENCIA;

GRANT DELETE, INSERT, REFERENCES, SELECT, UPDATE
 ON ADUENVIOSALIDA TO  SYSDBA WITH GRANT OPTION;
